<!--
-*****************************************************************************
                        CAN Participant QoS Profiles
-*****************************************************************************
-->
<dds xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="http://community.rti.com/schema/7.3.0/rti_dds_qos_profiles.xsd">

    <qos_library name="CANParticipantQosLib">

        <!-- CANComponent : component specific QosProfile -->
        <qos_profile name="CANComponent" base_name="DomainQosLib::CAN">
            <participant_qos>
                <participant_name>
                    <name>CAN Component</name>
                    <role_name>CAN Component</role_name>
                </participant_name>
            </participant_qos>
        </qos_profile>

    </qos_library>
</dds>
