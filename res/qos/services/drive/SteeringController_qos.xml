<?xml version="1.0" encoding="UTF-8"?>
<dds xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="http://community.rti.com/schema/7.3.0/rti_dds_qos_profiles.xsd">

  <configuration_variables>
    <value>
      <element>
        <name>STEERING_CONTROLLER_STRENGTH</name>
        <value>1</value>
      </element>
    </value>
  </configuration_variables>

  <qos_library name="DriveQosLib">
    <qos_profile name="SteeringController" base_name="DriveQosLib::_root_"
      is_default_participant_factory_profile="true">
      <base_name>
        <element>SnippetQosLib::Factory.NoAutoEnable</element>
      </base_name>

      <datawriter_qos base_name="FlowQosLib::Command" topic_filter="Command/Steering">
        <ownership_strength>
          <value>10</value>
        </ownership_strength>
      </datawriter_qos>

      <domain_participant_qos>
        <participant_name>
          <name>SteeringController</name>
        </participant_name>
      </domain_participant_qos>
    </qos_profile>
  </qos_library>

</dds>
